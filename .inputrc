# enable Alt-arrows
#"\e[1;3D": shell-backward-word ### Alt left
#"\e[1;3C": shell-forward-word  ### Alt right
"\ek": previous-history
"\ej": next-history

"\C-p": history-search-backward
"\C-n": history-search-forward
"\C-h": shell-backward-word
"\C-l": shell-forward-word
#"\C-o": quoted-insert

# vi mode in prompt
set show-mode-in-prompt on
set vi-ins-mode-string \1\e[36;1m\2(ins) \1\e[0m\2
set vi-cmd-mode-string \1\e[35;1m\2(cmd) \1\e[0m\2
